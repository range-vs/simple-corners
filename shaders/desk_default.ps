Texture2D singleTexture : register(t0);
SamplerState singleSampler : register(s0);

struct VS_INPUT
{
	float4 pos: SV_POSITION;
	float2 texcoord: TEXCOORD0;
};

float4 main(VS_INPUT input): SV_Target
{
	float4 color = singleTexture.Sample(singleSampler, input.texcoord);
	clip(color.a - 0.3f);
	return color;
	//return color * textColor;
}